            , A.upid as "upid"
            , A.updt as "updt"
            , B.gwaninname as "gwaninname"
            , B.ingubun as "ingubun"
            , B.regdt as "regdt"
            , B.firstdt as "firstdt"
            , B.regreason as "regreason"
            , B.manageorgid as "manageorgid"
            , B.regetc as "regetc"
            , B.deletedt as "deletedt"
            , B.deletereason as "deletereason"
            , B.deleteorgid as "deleteorgid"
            , B.deletegrade as "deletegrade"
            , B.deleteuserid as "deleteuserid"
            , B.deleteetc as "deleteetc"
            , C.orgname as "orgname" 
            , C.parentorgid as "parentorgid" 
            , C.orgtype as "orgtype" 
            , C.orgorder as "orgorder" 
            , C.toporgid as "toporgid" 
            , C.displayname as "displayname" 
            , C.descriptionid as "descriptionid" 
            , C.whencreated as "whencreated" 
            , C.whendeleted as "whendeleted" 
            , C.isdeleted as "isdeleted" 
            , C.chiefid as "chiefid" 
            , C.chiefposition as "chiefposition" 
            , C.suborgtype as "suborgtype" 
            , C.orgkind as "orgkind" 
            , C.relaytype as "relaytype" 
            , C.docdeptid as "docdeptid" 
            , C.presedeptid as "presedeptid" 
            , C.transdeptid as "transdeptid" 
            , C.orgdepth as "orgdepth" 
            , C.docsysteminfo as "docsysteminfo" 
            , C.readtype as "readtype" 
            , C.belonguserid as "belonguserid"
            , D.username as "username"
        FROM  sbms.sbms_user.com_org_detail_info  A
            LEFT OUTER JOIN  sbms.sbms_user.com_org_gwaninfo  B
                ON ( A.orgid  =  B.orgid  and  A.seq  =  B.seq )
            LEFT OUTER JOIN  sbms.sbms_user.com_organizationinfo  C
                ON ( A.orgid  =  C.orgid  )
            LEFT OUTER JOIN  sbms.sbms_user.com_userinfo  D
                ON ( A.inid  =  D.userid  )
        WHERE 1 = 1
            <if test="orgid != null and orgid !=''">
                and A.orgid = #{orgid}
            </if>
            <if test="gwaninname != null and gwaninname !=''"> 
                and gwaninname like CONCAT('%', #{gwaninname}, '%') 
            </if>
            <if test="etc != null and etc !=''"> 
                and etc like CONCAT('%', #{etc}, '%') 
            </if>
            <if test="orgname != null and orgname !=''"> 
                and orgname like CONCAT('%', #{orgname}, '%') 
            </if>
            <if test="managegubun != null and managegubun !=''">
                and ( A.managegubun = 'ORGE1' or A.managegubun = 'ORGE3' )
            </if>
            <if test="startDt != null and startDt !=''">
                and A.indt <![CDATA[>=]]> #{startDt}
            </if>
            <if test="endDt != null and endDt !=''">
                and A.indt <![CDATA[<=]]> #{endDt}
            </if>
        ORDER BY indt DESC
    </select>


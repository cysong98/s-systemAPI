    <select id="selectGwanList" parameterType="com.anylogic.ssystem.bms.com.model.SelectGwanListReqVO" resultMap="selectGwanListMap">
        SELECT
            A.orgid as "orgid"
            , A.seq as "seq"
            , A.managegubun as "managegubun"
